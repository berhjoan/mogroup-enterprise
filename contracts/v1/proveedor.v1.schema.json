{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://mogroup.local/schemas/proveedor.v1.json",
  "title": "Proveedor",
  "type": "object",
  "required": ["id","alias","contacto","activo"],
  "properties": {
    "id": { "type":"string" },
    "alias": { "type":"string" },
    "contacto": { "type":"object", "properties":{
      "nombre":{ "type":"string" },
      "email":{ "type":"string","format":"email" },
      "telefono":{ "type":"string" }
    }, "additionalProperties": false },
    "activo": { "type":"boolean" }
  },
  "additionalProperties": false
}
